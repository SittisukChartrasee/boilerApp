{
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": 8
  },
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-filename-extension": [ 1, { 
      "extensions": ["", ".js", ".jsx"]
    }],
    "react/forbid-prop-types": [2, { "forbid": [], "checkContextTypes": false, "checkChildContextTypes": false }],
    "jsx-a11y/anchor-has-content": [ 2, {
      "components": [ "Anchor" ]
    }],
    "semi":[2, "never"],
    "quotes": [2, "single"],
    "no-tabs": [2, "never"],
    "import/no-unresolved": "off"
  }
}